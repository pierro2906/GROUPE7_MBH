{
  "name": "i-signal",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "./build/src/index.js",
  "scripts": {
    "prebuild": "rimraf ./build",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "copyfiles": "copyfiles -u 1 public/**/*.* build/public/ && copyfiles -u 1 src/views/**/*.* build/src/ && copyfiles -u 1 src/utils/nodemailer/templates/**/*.* build/src/",
    "start:prod": "npm run prebuild && npm run build && npm run copyfiles && cross-env NODE_ENV=production node build/src/app.js",
    "start": "ts-node src/index.ts",
    "dev": "nodemon",
    "prisma:seed": "ts-node prisma/client/seed.ts",
    "prisma:migrate": "dropdb miabe && createdb miabe && prisma migrate dev --name init",
    "prisma:reset": "prisma migrate reset",
    "prisma:reset:force": "prisma migrate reset -f",
    "db:seed": "psql --dbname=miabe --file=./prisma/client/seed.sql",
    "db:repopulate": "npm run prisma:migrate && npm run db:seed",
    "make:service": "ts-node  -r tsconfig-paths/register src/scripts/generate-service",
    "make:daos": "ts-node  -r tsconfig-paths/register src/scripts/generate-dao",
    "make:schema": "ts-node  -r tsconfig-paths/register src/scripts/generate-schema",
    "make:controller": "ts-node  -r tsconfig-paths/register src/scripts/generate-controller",
    "make:routes": "ts-node  -r tsconfig-paths/register src/scripts/generate-routes"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "author": "AMONA Birewa AUdrey",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@vonage/server-sdk": "^3.14.0",
    "ansi-colors": "^4.1.3",
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.1",
    "commander": "^12.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "otp-generator": "^4.0.1",
    "uuid": "^9.0.1",
    "xss-filters": "^1.2.7",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.8",
    "@types/nodemailer": "^6.4.14",
    "@types/otp-generator": "^4.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@types/xss-filters": "^0.0.30",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concurrently": "^8.2.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "rimraf": "^5.0.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^5.4.5"
  }
}
